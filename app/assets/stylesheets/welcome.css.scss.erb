/* HTML5 Boilerplate  */

article, aside, details, figcaption, figure, footer, header, hgroup, nav, section { display: block; }
audio, canvas, video { display: inline-block; *display: inline; *zoom: 1; }
audio:not([controls]) { display: none; }
[hidden] { display: none; }

html { font-size: 100%; -webkit-text-size-adjust: 100%; -ms-text-size-adjust: 100%; }
html, button, input, select, textarea { font-family: sans-serif; color: #222; }
body { margin: 0; font-size: 1em; line-height: 1.4; }

::-moz-selection { background: #00aaff; color: #fff; text-shadow: none; }
::selection { background: #00aaff; color: #fff; text-shadow: none; }

abbr[title] { border-bottom: 1px dotted; }
b, strong { font-weight: bold; }
blockquote { margin: 1em 40px; }
dfn { font-style: italic; }
hr { display: block; height: 1px; border: 0; border-top: 1px solid #ccc; margin: 1em 0; padding: 0; }
ins { background: #ff9; color: #000; text-decoration: none; }
mark { background: #ff0; color: #000; font-style: italic; font-weight: bold; }
pre, code, kbd, samp { font-family: monospace, serif; _font-family: 'courier new', monospace; font-size: 1em; }
pre { white-space: pre; white-space: pre-wrap; word-wrap: break-word; }

q { quotes: none; }
q:before, q:after { content: ""; content: none; }
small { font-size: 85%; }
sub, sup { font-size: 75%; line-height: 0; position: relative; vertical-align: baseline; }
sup { top: -0.5em; }
sub { bottom: -0.25em; }

ul, ol { margin: 1em 0; padding: 0 0 0 40px; }
dd { margin: 0 0 0 40px; }
nav ul, nav ol { list-style: none; list-style-image: none; margin: 0; padding: 0; }

img { border: 0; -ms-interpolation-mode: bicubic; vertical-align: middle; }
svg:not(:root) { overflow: hidden; }
figure { margin: 0; }

form { margin: 0; }
fieldset { border: 0; margin: 0; padding: 0; }

label { cursor: pointer; }
legend { border: 0; *margin-left: -7px; padding: 0; white-space: normal; }
button, input, select, textarea { font-size: 100%; margin: 0; vertical-align: baseline; *vertical-align: middle; }
button, input { line-height: normal; }
button, input[type="button"], input[type="reset"], input[type="submit"] { cursor: pointer; -webkit-appearance: button; *overflow: visible; }
button[disabled], input[disabled] { cursor: default; }
input[type="checkbox"], input[type="radio"] { box-sizing: border-box; padding: 0; *width: 13px; *height: 13px; }
input[type="search"] { -webkit-appearance: textfield; -moz-box-sizing: content-box; -webkit-box-sizing: content-box; box-sizing: content-box; }
input[type="search"]::-webkit-search-decoration, input[type="search"]::-webkit-search-cancel-button { -webkit-appearance: none; }
button::-moz-focus-inner, input::-moz-focus-inner { border: 0; padding: 0; }
textarea { overflow: auto; vertical-align: top; resize: vertical; }
input:valid, textarea:valid {  }
input:invalid, textarea:invalid { background-color: #f0dddd; }

table { border-collapse: collapse; border-spacing: 0; }
td { vertical-align: top; }

.chromeframe { margin: 0.2em 0; background: #ccc; color: black; padding: 0.2em 0; }

@font-face {
  font-family: FontAwesome;
  src: font-url('custom-fontawesome-webfont.eot'); /* IE9 Compat Modes */
  src: font-url('custom-fontawesome-webfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       font-url('custom-fontawesome-webfont.woff') format('woff'), /* Pretty Modern Browsers */
       font-url('custom-fontawesome-webfont.ttf') format('truetype'), /* Safari, Android, iOS */
       font-url('fontawesome-webfont.svg#FontAwesome') format('svg'); /* Legacy iOS */
}

  /*  Font Awesome styles
      ------------------------------------------------------- */
  [class^="icon-"]:before, [class*=" icon-"]:before {
    font-family: FontAwesome;
    font-weight: normal;
    font-style: normal;
    display: inline-block;
    text-decoration: inherit;
    -webkit-font-smoothing: antialiased;
  }
  .icon-user:before		{ content: "\f021"; }
  .icon-plus-sign:before		{ content: "\f022"; }
  .icon-remove-sign:before		{ content: "\f023"; }
  .icon-remove-circle:before		{ content: "\f024"; }
  .icon-ok-circle:before		{ content: "\f025"; }
  .icon-calendar:before		{ content: "\f026"; }
  .icon-rss:before		{ content: "\f027"; }
  .icon-envelope-alt:before		{ content: "\f028"; }

  .carousel-inner {
    position: relative;
  }

  .carousel {
    .item {
      display: block;
      position: static;
      transition: 0.6s ease-in-out left;
      > img {
        display: block;
        line-height: 1;
      }
    }
  }


  .main .container, .carousel .container, .main_section, .object_container, .regular_section nav li a {
    box-sizing: border-box;
  }

  body, input, textarea {
    font-family: "Imprima", sans-serif;
    font-size: 16px;
  }

  a {
    text-decoration: none;
    color: #0080ff;
  }

  a:hover {
    color: #11a0ff;
  }

  nav a, input[type="submit"] {
    transition: background-color 1s, transform .2s;
  }

  nav a {
    &:hover, .active {
      transform: scale(1.1);
    }
  }

  input[type="submit"] {
    &:hover {
      transform: scale(1.05);
    }
  }

  .page_header, #welcome {
    h1 {
      transform: skew(0,-10deg);
    }
  }
  .page_header {
    position: fixed;
    z-index: 2;
    width: 100%;
    background: linear-gradient(top, rgba(255,255,255,.75), rgba(255,255,255,.25));
    filter: progid:DXImageTransform.Microsoft.Gradient(startColorStr=#{ie_hex_str(rgba(255,255,255,.75))}, endColorStr=#{ie_hex_str(rgba(255,255,255,.25))});
    a {
      color: #333;
    }
    h1 {
      text-align: center;
      font-size: 12px;
      float: left;
      width: 25%;
    }
    nav {
      width: 75%;
      float: left;
    }
    li {
      float: left;
      width: 25%;
      &.active:after {
        content: " ";
        width: 0;
        height: 0;
        border-bottom: 8px solid #333;
        border-left: 8px solid transparent;
        border-right: 8px solid transparent;
        display: block;
        margin: -8px auto 0;
      }
      a {
        display: block;
        text-align: center;
        line-height: 76px;
        font-weight: bold;
      }
      i {
        font-size: 18px;
      }
    }
  }

  .link_text {
    display: none;
  }

  h1, h2 {
    font-family: 'Carter One', sans-serif;
    font-weight: normal;
    .jeffrey, .anna, .and {
      display: block;
    }
    .jeffrey, .anna {
      font-size: 2em;
    }
    .jeffrey {
      line-height: 1.2em;
    }
    .anna {
      line-height: 1em;
    }
    .and {
      line-height: .7em;
      &:before, &:after {
        content: " - ";
      }
    }
    .ampersand {
      display: none;
    }
  }

  $container-max-width: 800px;
  .container {
    margin: 0 auto;
    max-width: $container-max-width;
    position: relative;
  }

  $container-horiz-padding: 20px;
  .main .container {
    padding: 0 $container-horiz-padding;
  }

  .carousel {
    height: 75%;
    bottom: 4%;
    clear: both;
    .carousel-inner, .item, .container {
      height: 100%;
    }
    .container {
      padding: 10px $container-horiz-padding;
      overflow: auto;
      //box-shadow: 0 0 30px rgba(0,0,0,.25);
      img.size-large.alignnone {
        display: block;
        margin: 0 auto;
        width: 100%;
        height: auto;
      }
      table {
        width: 100%;
        td {
          padding: 5px 0;
        }
        tr:nth-child(odd) {
          background: rgba(218,218,255,.25);
        }
        td:last-child {
          text-align: right;
        }
      }
      h1 a, a.hotel_heading {
        color: inherit;
      }
      h1 {
        font-size: 2em;
        margin: .5em 0;
        line-height: 1.4em;
      }
      section h1 {
        margin: 1em 0;
        font-size: 1.5em;
      }
    }
  }

  .main_section {
    width: 100%;
    min-height: 800px;
    position: relative;
    overflow: hidden;
    header {
      top: 10%;
      h1 {
        font-size: 40px;
      }
      h2 {
        font-size: 30px;
        margin: .15em 0;
      }
      h3 {
        font-size: 25px;
        margin: .35em 0;
      }
    }
  }

  .content_section {
    padding-top: 77px;
  }

  .blog_metadata {
    font-size: .9em;
    font-style: italic;
  }

  .blog_date {
    a {
      color: inherit;
    }
  }

  .blog_link {
    text-align: right;
  }

  input[type="submit"], .regular_section nav li a {
    font-weight: bold;
    border-radius: 10px;
  }

  .regular_section {
    nav {
      display: none;
      float: right;
      margin-top: 29px;
      li {
        float: left;
        margin-left: 20px;
        a {
          line-height: 25px;
          padding: 10px;
          display: block;
        }
      }
    }
  }

  $welcome-heading-shadow-color: #081073;
  #welcome {
    background: #ffda73 image-url('backgrounds/welcome.jpg');
    header {
      top: 25%;
    }
    h1, h2, h3 {
      text-align: center;
      color: #7279da;
    }
    h1, h3 {
      text-shadow: .05em .1em 0 $welcome-heading-shadow-color;
    }
    h1 {
      margin-bottom: 0;
    }
    h2 {
      text-shadow: .05em .1em 0 $welcome-heading-shadow-color, 0 0 20px white;
    }
  }

  @mixin regular-section-styles($color-1, $color-2, $color-3, $color-4, $color-5) {
    nav a {
      background: transparentize($color-1, .15);
    }
    .item .container {
      background: transparentize(lighten($color-1, 20%), .15);
    }
    header, header a {
      color: $color-2;
    }
    header h1 {
      text-shadow: .05em .1em 0 $color-3;
    }
    nav {
      a:hover {
        background: $color-4;
      }
      a.active {
        background: $color-5;
      }
    }
  }

  $about-us-color: #d4f870;
  #about_us, #about_us nav a, #about_us .item .container {
    background-color: $about-us-color;
  }
  #about_us {
    @include regular-section-styles($about-us-color, #e065bb, #7d0057, #c7f83e, #b1f100);
    background-image: image-url('backgrounds/about_us.jpg');
  }

  $updates-color: #61b7cf;
  #updates, #updates nav a, #updates .item .container {
    background-color: $updates-color;
  }
  #updates {
    @include regular-section-styles($updates-color, #ffba73, #a65400, #39aecf, #057d9f);
    background-image: image-url('backgrounds/updates.jpg');
  }

  $events-color: #9c6ad6;
  #events, #events nav a, #events .item .container {
    background-color: $events-color;
  }
  #events {
    @include regular-section-styles($events-color, #fff573, #a69a00, #8742d6, #580EAD);
    background-image: image-url('backgrounds/events.jpg');
  }

  $rsvp-color: #ff7373;
  $rsvp-hover-color: #ff4040;
  $rsvp-active-color: #ff0000;
  #rsvp, #rsvp nav a, #rsvp .item .container {
    background-color: $rsvp-color;
  }
  #rsvp {
    @include regular-section-styles($rsvp-color, #67e667, #008500, $rsvp-hover-color, $rsvp-active-color);
    background-image: image-url('backgrounds/rsvp.jpg');
  }

  .form_row {
    margin: 1em 0;
  }

  .carousel fieldset.container {
    padding-top: 20px;
    margin-bottom: 1em;
  }

  legend {
    font-weight: bold;
  }

  label {
    display: inline-block;
    width: 140px;
  }

  form.new_invitation {
    text-align: center;
    input[type="text"] {
      font-size: 3em;
      width: 190px;
      margin-right: .5em;
    }
  }

  input[type="submit"] {
    font-size: 2.7em;
  }

  form.edit_invitation {
    clear: both;
  }

  .flash {
    font-weight: bold;
  }

  .badge {
    display: block;
    float: left;
    font-size: 80px;
    height: 80px;
    margin: 8px 20px 20px 0;
    text-align: center;
    width: 80px;
    &.icon-ok-circle {
      color: green;
    }
    &.icon-remove-circle {
      color: red;
    }
  }

  input[type="submit"], .remove_nested_fields, .add_nested_fields {
    background-color: $rsvp-color;
    border: none;
    padding: 5px 10px;
    &:hover {
      background-color: $rsvp-hover-color;
    }
    &:active {
      background-color: $rsvp-active-color;
    }
  }

  .remove_nested_fields, .add_nested_fields {
    border-radius: 5px;
    color: inherit;
    &:hover {
      color: inherit;
    }
  }

  .remove_nested_fields {
    float: right;
  }

  .container_opacity {
    display: none;
  }

  #about_us img {
    width: auto;
  }
  .image_right {
    float: right;
    margin: 0 0 1em 1em;
  }
  .image_left {
    float: left;
    margin: 0 1em 1em 0;
  }

  .hotel_heading {
    display: block;
    h2 {
      background-position: 50% 100%;
      background-repeat: no-repeat;
      box-sizing: border-box;
      padding-bottom: 210px;
    }
  }

  .marriott_heading h2 {
    background-image: image-url('/marriott.jpg')
  }

  .kensington_heading h2 {
    background-image: image-url('/kensington.jpg')
  }

  @media only screen and (max-width: 400px) {
    #about_us img {
      max-width: 100%;
      height: auto;
      float: none;
    }
  }

  $mobile-width: 710px;
  @media only screen and (max-width: $mobile-width - 1) {
    .page_header {
      h1 {
        margin: 0;
        .and, .not_first_letter {
          display: none;
        }
        .ampersand {
          display: block;
          margin: -1em 0 0 2em;
        }
        .anna {
          margin: -.5em 0 0 2em;
        }
      }
      li a {
        line-height: 44px;
      }
    }
    .main_section {
      box-sizing: border-box;
      padding-top: 45px;
    }
    .author {
      display: none;
    }
  }

  @media only screen and (min-width: $mobile-width) {
    .page_header li [class^="icon-"]:before, .page_header li [class*=" icon-"]:before {
      box-sizing: border-box;
      padding-right: .5em;
    }
    .link_text {
      display: inline;
    }
    .main_section header, .carousel {
      width: 100%;
      z-index: 1;
      position: absolute;
    }
    .regular_section {
      header h1 {
        float: left;
      }
      nav {
        display: block;
      }
    }
    .carousel {
      .item {
        display: none;
        position: relative;
      }
      .active, .next, .prev {
        display: block;
      }
      .active {
        left: 0;
      }
      .next, .prev {
        position: absolute;
        top: 0;
        width: 100%;
      }
      .next {
        left: 100%;
      }
      .prev {
        left: -100%;
      }
      .next.left, .prev.right {
        left: 0;
      }
      .active {
        &.left {
          left: -100%;
        }
        &.right {
          left: 100%;
        }
      }
    }
  }

  @media only screen and (min-width: $mobile-width) and (min-height: 601px) {
    .main_section header {
      h1 {
        margin: 0;
        font-size: 60px;
      }
      h2 {
        font-size: 50px;
      }
      h3 {
        font-size: 40px;
      }
    }
  }

